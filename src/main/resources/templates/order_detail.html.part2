<!-- JS inline: g·ª≠i ƒë√°nh gi√° k√®m CSRF -->
<script>
document.addEventListener('DOMContentLoaded', function () {
  const btn = document.getElementById('btnSubmitReviews');
  if (!btn) return;

  const csrfToken  = document.querySelector('meta[name="_csrf"]')?.getAttribute('content');
  const csrfHeader = document.querySelector('meta[name="_csrf_header"]')?.getAttribute('content');

  btn.addEventListener('click', async function (e) {
    e.preventDefault();

    const orderId = btn.dataset.oid;
    const blocks = Array.from(document.querySelectorAll('#reviewModal .row.g-3'));

    const headers = { 'Content-Type': 'application/json' };
    if (csrfToken && csrfHeader) headers[csrfHeader] = csrfToken;

    let ok = 0, fail = 0;

    for (const block of blocks) {
      const textarea = block.querySelector('textarea');
      const select   = block.querySelector('select.rating');
      if (!textarea || !select) continue;

      const laptopId = textarea.dataset.pid;
      const comment  = textarea.value.trim();
      const rating   = parseInt(select.value, 10) || 5;

      if (!laptopId || !comment) continue;

      try {
        const res = await fetch(`/reviews/orders/${orderId}/laptops/${laptopId}`, {
          method: 'POST',
          headers,
          body: JSON.stringify({ rating, comment })
        });
        if (res.ok) {
          ok++;
        } else {
          fail++;
          console.error('Send review failed:', res.status, await res.text());
        }
      } catch (err) {
        fail++;
        console.error('Network/JS error while sending review:', err);
      }
    }

    if (ok > 0 && fail === 0) {
      window.location.reload();
    } else if (ok > 0 && fail > 0) {
      alert(`M·ªôt s·ªë ƒë√°nh gi√° g·ª≠i th·∫•t b·∫°i. Th√†nh c√¥ng: ${ok}, L·ªói: ${fail}. M·ªü Console ƒë·ªÉ xem chi ti·∫øt.`);
    } else {
      alert('Kh√¥ng c√≥ ƒë√°nh gi√° n√†o ƒë∆∞·ª£c g·ª≠i. Ki·ªÉm tra l·∫°i n·ªôi dung ho·∫∑c m·ªü Console ƒë·ªÉ xem l·ªói.');
    }
  });
});
</script>
</body>
